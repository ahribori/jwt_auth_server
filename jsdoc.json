  {
    "tags": {
      "allowUnknownTags": false
    },
    "source": {
      "include": [
        "./sdk/core/api/v1.0/api.js"
      ],
      "includePattern": ".js$",
      "exclude": [
        "./docs"
      ],
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown",
      "node_modules/jsdoc-babel"
    ],
    "opts": {
      "template": "node_modules/docdash",
      "encoding": "utf8",
      "destination": "docs/",
      "recurse": true,
      "verbose": true
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": false
      }
    },
    "docdash": {
      "static": false,
      "sort": false
    }
  }